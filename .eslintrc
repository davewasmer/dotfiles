{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
    "qunit": true,
    "embertest": true
  },
  "extends": "airbnb/base",
  "rules": {
    "array-bracket-spacing": [ 2, "always" ],
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "indent": [ 0, 4 ],
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-rest-params": 0,
    "prefer-template": 0,
    "space-before-function-paren": 0,
    "vars-on-top": 0
  }
}
